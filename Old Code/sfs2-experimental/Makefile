# Project Setup
EXECNAME =	sfs
SRCDIR =	src
TSTDIR =	test
OBJDIR =	obj
BINDIR =	bin

# Compiler
CC =	gcc

# Compiler Flags
CFLAGS =	-ggdb -std=c99 -Wall

# Linker Flags
LDFLAGS =	

############################################################
DEPS=$(OBJDIR)/test.o  $(OBJDIR)/sfs.o  $(OBJDIR)/sfs_bitmap.o  $(OBJDIR)/sfs_queue.o  $(OBJDIR)/sfs_crc.o

all: $(DEPS)
	$(CC) -o $(BINDIR)/$(EXECNAME) $(DEPS) $(LDFLAGS)

$(OBJDIR)/%.o: $(SRCDIR)/%.c
	$(CC) $(CFLAGS) -c -o $@ $<

$(OBJDIR)/%.o: $(TSTDIR)/%.c
	$(CC) $(CFLAGS) -I$(SRCDIR) -c -o $@ $<

clean:
	find $(OBJDIR) -name "*.o" -print0 | xargs -0 rm -rf

cleaner: clean
	rm -f $(BINDIR)/$(EXECNAME)
